{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"]
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "cosmos",
      "databaseName": "hundred-and-ten-db",
      "collectionName": "game",
      "createIfNotExists": "true",
      "connectionStringSetting": "CosmosDb"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
